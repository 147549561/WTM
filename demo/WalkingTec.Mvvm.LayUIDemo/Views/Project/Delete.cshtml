@model WalkingTec.Mvvm.LayUIDemo.ViewModels.ProjectVMs.ProjectVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.ProjectNo" />
<wt:display field="Entity.ProjectName" />
<wt:display field="Entity.Province.ProvinceName" />
<wt:display field="Entity.City.CityName" />
<wt:display field="Entity.ContractorA.ContractorName" />
<wt:display field="Entity.ContractorB.ContractorName" />
<wt:display field="Entity.ContractIntroduction" />
<wt:display field="Entity.ContractDocumentID" />
<wt:display field="Entity.ContractSignDate" />
<wt:display field="Entity.ContractEndDate" />
<wt:display field="Entity.ContractAmount" />
<wt:display field="Entity.CashPledge" />
<wt:display field="Entity.SpecifiedBudgeAmount" />
<wt:display field="Entity.EstimateBudgeAmount" />
<wt:display field="Entity.Status" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>